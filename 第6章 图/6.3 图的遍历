bool visited[MAX_VERTEX_NUM]
void BFXTraverse(Graph G)
{
    for(i=0;i<G.vexnum;++i)
        visited[i]=FALSE;
    InitQueue(Q);
	for(i=0;i<G.vexnum;++i)
		if(!visited[i])
			BFS(G,i);
}
